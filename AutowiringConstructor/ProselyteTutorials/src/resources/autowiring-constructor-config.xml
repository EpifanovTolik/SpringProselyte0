<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- Definition for exam bean -->
    <bean id="exam" class="net.proselyte.autowiring.Exam"
          autowire="constructor">
        <constructor-arg value="Computer Science"/>
    </bean>

    <!-- Definition for answerChecker bean -->
    <bean id="answerChecker" class="net.proselyte.autowiring.AnswerChecker">
    </bean>

</beans>